{
    "installCommand": "npm install --legacy-peer-deps",
    "version":2,
    "functions":{
        "api/server.js":{
            "maxDuration":60
        }
    },
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"src/**/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {"src":"/(.*)","dest":"/"}
    ]
}