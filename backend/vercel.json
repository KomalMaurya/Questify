{
    "installCommand": "npm install --legacy-peer-deps",
    "version":2,
    "functions":{
        "api/server.js":{
            "memory":1024,
            "maxDuration":60
        }
    },
    "routes":[
        {
            "handle":"filesystem"
        },
        {
            "src":"/(.*)",
            "dest":"/index.js"}
    ]
}